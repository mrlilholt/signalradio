# Cloudinary credentials (do not commit real secrets)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# Cloudinary stores audio uploads under the "video" resource type.
CLOUDINARY_FOLDER=signal-radio

# Script behavior
PLAYLIST_FILE=src/features/stream/data_playlist_tracks.ts
MANIFEST_FILE=scripts/cloudinary_upload_manifest.json
SKIP_UPLOADED=true
MAX_RETRIES=4
